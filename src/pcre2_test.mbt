///|
test "captures" {
  let code = @pcre2.compile!("(?<hello>hellğ·o)")
  let subject = "a hellğ·o, ä½ å¥½ ğ· world! hellğ·o"
  let match_ = code.matches!(subject)
  let views = []
  while match_.next!() is Some(match_) {
    for _, capture in match_.captures() {
      views.push(capture.view().to_string())
    }
  }
  code.free()
  @json.inspect!(views, content=["hellğ·o", "hellğ·o"])
}
